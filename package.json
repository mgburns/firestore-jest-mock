{
  "name": "firestore-jest-mock",
  "version": "0.21.0",
  "description": "Jest helper for mocking Google Cloud Firestore",
  "author": "",
  "license": "MIT",
  "keywords": [
    "Jest",
    "Firestore"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.js"
  },
  "types": "index.d.ts",
  "scripts": {
    "build": "rm -fr dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup",
    "lint": "eslint . --ext .js,.ts",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage"
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Upstatement/firestore-jest-mock.git"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.2",
    "@react-native-firebase/firestore": "^15.1.1",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@upstatement/eslint-config": "^1.0.0",
    "@upstatement/prettier-config": "^0.3.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^29.5.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^27.2.1",
    "firebase": "^8.1.2",
    "firebase-admin": "^9.4.1",
    "jest": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^10.0.2",
    "prettier": "^2.x.x",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
